{
  "$schema": "https://dl.viam.dev/module.schema.json",
  "module_id": "hipsterbrown:pi-ai-camera",
  "visibility": "public",
  "url": "https://github.com/hipsterbrown/viam-pi-ai-camera",
  "description": "Modular vision service: pi-ai-camera",
  "build": {
    "build": "mise run package",
    "path": "module.tar.gz",
    "arch": [
      "linux/arm64"
    ]
  },
  "models": [
    {
      "api": "rdk:service:vision",
      "model": "hipsterbrown:vision:pi-ai-camera",
      "short_description": "Run computer vision tasks directly on the Raspberry Pi AI camera.",
      "markdown_link": "README.md#configure-your-pi-ai-camera-vision-service"
    }
  ],
  "entrypoint": "./run.sh",
  "first_run": "./setup.sh"
}
